{
  "name": "awsmug/torro-forms",
  "description": "Torro Forms is an extendable WordPress form builder with Drag & Drop functionality, chart evaluation and more - with WordPress look and feel.",
  "version": "1.0.0-beta.8",
  "license": "GPL-3.0",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "plugin",
    "forms",
    "form builder",
    "surveys",
    "polls",
    "votes",
    "charts",
    "api"
  ],
  "homepage": "http://torro-forms.com",
  "authors": [
    {
      "name": "Sven Wagener",
      "email": "contact@awesome.ug",
      "homepage": "http://www.awesome.ug",
      "role": "Product Lead"
    },
    {
      "name": "Felix Arntz",
      "email": "felix-arntz@leaves-and-love.net",
      "homepage": "https://leaves-and-love.net",
      "role": "Lead Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/awsmug/torro-forms/issues"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "easydigitaldownloads/EDD-License-handler",
        "version": "1.6.11",
        "source": {
          "url": "git://github.com/easydigitaldownloads/EDD-License-handler.git",
          "type": "git",
          "reference": "master"
        },
        "dist": {
          "url": "https://github.com/easydigitaldownloads/EDD-License-handler/archive/master.zip",
          "type": "zip"
        }
      }
    }
  ],
  "config": {
    "preferred-install": "dist"
  },
  "require": {
    "composer/installers": "~1.0",
    "felixarntz/plugin-lib": "dev-master",
    "phpoffice/phpexcel": "1.8.1",
    "easydigitaldownloads/EDD-License-handler": "1.6.11"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "dev-master"
  },
  "scripts":     {
    "post-install-cmd": [
      "cd vendor/felixarntz/plugin-lib",
      "npm install --only=production",
      "cd ../../.."
    ],
    "post-update-cmd": [
      "cd vendor/felixarntz/plugin-lib",
      "npm install --only=production",
      "cd ../../.."
    ]
  },
  "extra": {
    "installer-paths": {
      "vendor/felixarntz/{$name}/": ["felixarntz/plugin-lib"]
    }
  }
}
